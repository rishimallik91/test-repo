git init --> for initialising sample git repository
git config --global user.name "<username>"
git config --global user.email "<email id>"
create some new files f1 f2 f3 f4
git status
git add * / git add <file name>
git status
git commit -m "committed by <user>" <filename>
git status
git clone https://github.com/rishimallik91/sample.git
git clone https://github.com/rishimallik91/test-repo.git
git remote add test-repo https://github.com/rishimallik91/test-repo.git
git push --set-upstream test-repo master
git remote add origin <url>
git remote -v
git push -u origin master
